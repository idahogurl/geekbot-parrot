{
  "name": "geekbot-parrot",
  "version": "2.0.2",
  "description": "Answer the geekbot bot automatically with Github data",
  "scripts": {
    "cli-start": "node index --user idahogurl --organizations healthline",
    "cli-dev": "nodemon index --user idahogurl --organizations healthline",
    "start": "node ./dist/www.js",
    "dev": " nodemon ./dist/www.js",
    "watch-client": "parcel watch ./web/index.html --out-dir ./dist/public",
    "watch-server": "parcel watch ./web/bin/www.js --target node",
    "build-client": "parcel build ./web/index.html --out-dir ./dist/public",
    "build-server": "parcel build ./web/bin/www.js --target node"
  },
  "dependencies": {
    "arg": "^2.0.0",
    "base-64": "^0.1.0",
    "chalk": "^2.4.1",
    "chrono-node": "^1.3.5",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.9.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jira-client": "^6.21.1",
    "moment-timezone": "^0.5.26",
    "morgan": "^1.10.0",
    "preact": "^10.4.6",
    "react-checkbox-list": "^0.0.2",
    "react-copy-to-clipboard": "^5.0.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/eslint-parser": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^2.0.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.13.7",
    "prettier-eslint": "^10.0.0"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "author": "",
  "license": "ISC",
  "bin": {
    "geekbot-parrot": "./bin/geekbot-parrot"
  }
}
